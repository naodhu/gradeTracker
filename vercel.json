{
  "version": 2,
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/auth",
      "dest": "/auth.html"
    },
    {
      "src": "/welcome",
      "dest": "/welcome.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
